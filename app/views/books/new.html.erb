 <div class="social"></div>

<h2>User info</h2>
<%= form_with model: @book, url:'/books', local:true do |f| %>
<%= f.attachment_field :image %>
<p>name</p>
<p>introduction</p>
<p><%= f.submit  %></p>
<% end %>

<h2>New book</h2>
<%= form_with model: @books, local:true do |f| %>
<p>Title</p>
<%= f.text_field :title %>
<p>Opinion</p>
 <%= f.text_area :body %>
<p><%= f.submit 'Create Book' %></p>
<% end %>